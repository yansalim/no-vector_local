{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "web": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend": "cd backend && ./start.sh",
    "backend:win": "cd backend && python -m pip install -r requirements.txt && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev": "concurrently -n backend,web -c blue,green \"npm:backend:win\" \"npm:web\"",
    "dev:all": "concurrently -n backend,web -c blue,green \"npm:backend:win\" \"npm:web\""
  },
  "dependencies": {
    "lucide-react": "^0.539.0",
    "openai": "^5.12.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^5.4.19"
  }
}
